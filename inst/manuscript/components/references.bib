}

@article{vanniel2018,
  title = {Shedding Light on the Cell Biology of Extracellular Vesicles},
  volume = {19},
  copyright = {2018 Nature Publishing Group},
  issn = {1471-0080},
  abstract = {Extracellular vesicles are a heterogeneous group of cell-derived membranous structures comprising exosomes and microvesicles, which originate from the endosomal system or which are shed from the plasma membrane, respectively. They are present in biological fluids and are involved in multiple physiological and pathological processes. Extracellular vesicles are now considered as an additional mechanism for intercellular communication, allowing cells to exchange proteins, lipids and genetic material. Knowledge of the cellular processes that govern extracellular vesicle biology is essential to shed light on the physiological and pathological functions of these vesicles as well as on clinical applications involving their use and/or analysis. However, in this expanding field, much remains unknown regarding the origin, biogenesis, secretion, targeting and fate of these vesicles.},
  language = {en},
  number = {4},
  journal = {Nat. Rev. Mol. Cell Biol.},
  doi = {10.1038/nrm.2017.125},
  author = {{van Niel}, Guillaume and D'Angelo, Gisela and Raposo, Gra{\c c}a},
  month = apr,
  year = {2018},
  pages = {213-228},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Honours\\Review EV\\van Niel et al_2018_Shedding light on the cell biology of extracellular vesicles.pdf;C:\\Users\\charl\\Zotero\\storage\\F53THRU3\\nrm.2017.html}
}

@article{sudbery2011,
  title = {Growth of {{{\emph{Candida}}}}{\emph{ Albicans}} Hyphae},
  volume = {9},
  copyright = {2011 Nature Publishing Group},
  issn = {1740-1534},
  abstract = {The fungus Candida albicans is often a benign member of the mucosal flora; however, it commonly causes mucosal disease with substantial morbidity and in vulnerable patients it causes life-threatening bloodstream infections. A striking feature of its biology is its ability to grow in yeast, pseudohyphal and hyphal forms. The hyphal form has an important role in causing disease by invading epithelial cells and causing tissue damage. This Review describes our current understanding of the network of signal transduction pathways that monitors environmental cues to activate a programme of hypha-specific gene transcription, and the molecular processes that drive the highly polarized growth of hyphae.},
  language = {en},
  number = {10},
  journal = {Nat. Rev. Microbiol.},
