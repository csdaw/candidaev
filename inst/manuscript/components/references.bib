  pages = {339-350},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Honours\\EV markers\\Candida 43\\Yu et al_2014_A novel role of the vacuolar calcium channel Yvc1 in stress response,.pdf;C:\\Users\\charl\\Zotero\\storage\\MVVY7B8E\\S1438422113001951.html}
}

