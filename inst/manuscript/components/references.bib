  pmid = {30232125}
}

@article{hammond2018,
  title = {Stable {{Isotope Dynamic Labeling}} of {{Secretomes}} ({{SIDLS}}) {{Identifies Authentic Secretory Proteins Released}} by {{Cancer}} and {{Stromal Cells}}},
