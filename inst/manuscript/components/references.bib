  volume = {13},
  copyright = {\textcopyright{} 2013 WILEY-VCH Verlag GmbH \& Co. KGaA, Weinheim},
  issn = {1615-9861},
  abstract = {Exosomes are small extracellular 40\textendash{}100 nm diameter membrane vesicles of late endosomal origin that can mediate intercellular transfer of RNAs and proteins to assist premetastatic niche formation. Using primary (SW480) and metastatic (SW620) human isogenic colorectal cancer cell lines we compared exosome protein profiles to yield valuable insights into metastatic factors and signaling molecules fundamental to tumor progression. Exosomes purified using OptiPrep\texttrademark{} density gradient fractionation were 40\textendash{}100 nm in diameter, were of a buoyant density {$\sim$}1.09 g/mL, and displayed stereotypic exosomal markers TSG101, Alix, and CD63. A major finding was the selective enrichment of metastatic factors (MET, S100A8, S100A9, TNC), signal transduction molecules (EFNB2, JAG1, SRC, TNIK), and lipid raft and lipid raft-associated components (CAV1, FLOT1, FLOT2, PROM1) in exosomes derived from metastatic SW620 cells. Additionally, using cryo-electron microscopy, ultrastructural components in exosomes were identified. A key finding of this study was the detection and colocalization of protein complexes EPCAM-CLDN7 and TNIK-RAP2A in colorectal cancer cell exosomes. The selective enrichment of metastatic factors and signaling pathway components in metastatic colon cancer cell-derived exosomes contributes to our understanding of the cross-talk between tumor and stromal cells in the tumor microenvironment.},
  language = {en},
  number = {10-11},
  journal = {PROTEOMICS},
  doi = {10.1002/pmic.201200562},
  author = {Ji, Hong and Greening, David W. and Barnes, Thomas W. and Lim, Justin W. and Tauro, Bow J. and Rai, Alin and Xu, Rong and Adda, Christopher and Mathivanan, Suresh and Zhao, Wei and Xue, Yanhong and Xu, Tao and Zhu, Hong-Jian and Simpson, Richard J.},
  month = may,
  year = {2013},
  keywords = {Exosomes,Secretome,Cancer,Metastasis,Cell biology,Protein complex},
  pages = {1672-1686},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Honours\\Proteomics\\Mammalian EV\\Ji et al_2013_Proteome profiling of exosomes derived from human primary and metastatic.pdf;C:\\Users\\charl\\Zotero\\storage\\ILLLWPQC\\pmic.html}
