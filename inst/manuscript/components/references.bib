  author = {Havlickova, Blanka and Czaika, Viktor A. and Friedrich, Markus},
  year = {2008},
  keywords = {Candida,epidemiology,Skin mycoses,tinea,Trichophyton},
  pages = {2-15},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Honours\\Candida pathogenesis\\Epidemiology\\Havlickova et al_2008_Epidemiological trends in skin mycoses worldwide.pdf;C:\\Users\\charl\\Zotero\\storage\\4GXLFZR6\\j.1439-0507.2008.01606.html}
}

@article{davis2002,
