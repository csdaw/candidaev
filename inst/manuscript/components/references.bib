  keywords = {amyotrophic lateral sclerosis,granules,heat shock,motor neuron disease,neurodegeneration,phase separation,ribonucleoprotein,RNA-binding proteins,stress},
  pages = {590-604.e13},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Cambridge application\\Stress granules\\Proteomics\\Markmiller et al_2018_Context-Dependent and Disease-Specific Diversity in Protein Interactions within.pdf;C:\\Users\\charl\\Zotero\\storage\\VBJ7ULJ7\\S009286741731512X.html}
