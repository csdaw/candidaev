  file = {C:\\Users\\charl\\OneDrive\\zotero\\Honours\\Tetraspanins\\CD81\\Bertaux_Dragic_2006_Different Domains of CD81 Mediate Distinct Stages of Hepatitis C Virus.pdf;C:\\Users\\charl\\Zotero\\storage\\HTXGMQF4\\4940.html},
  pmid = {16641285}
}

