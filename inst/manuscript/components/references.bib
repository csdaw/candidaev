  month = may,
  year = {2018},
  pages = {333336},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Cambridge application\\RBPs\\Research\\Queiroz et al_2018_Unbiased dynamic characterization of RNA-protein interactions by OOPS.pdf;C:\\Users\\charl\\Zotero\\storage\\ILFN2LH5\\333336.html}
}

@article{darnell2011,
  title = {{{FMRP Stalls Ribosomal Translocation}} on {{mRNAs Linked}} to {{Synaptic Function}} and {{Autism}}},
  volume = {146},
  issn = {0092-8674, 1097-4172},
  abstract = {{$<$}h2{$>$}Summary{$<$}/h2{$><$}p{$>$}FMRP loss of function causes Fragile X syndrome (FXS) and autistic features. FMRP is a polyribosome-associated neuronal RNA-binding protein, suggesting that it plays a key role in regulating neuronal translation, but there has been little consensus regarding either its RNA targets or mechanism of action. Here, we use high-throughput sequencing of RNAs isolated by crosslinking immunoprecipitation (HITS-CLIP) to identify FMRP interactions with mouse brain polyribosomal mRNAs. FMRP interacts with the coding region of transcripts encoding pre- and postsynaptic proteins and transcripts implicated in autism spectrum disorders (ASD). We developed a brain polyribosome-programmed translation system, revealing that FMRP reversibly stalls ribosomes specifically on its target mRNAs. Our results suggest that loss of a translational brake on the synthesis of a subset of synaptic proteins contributes to FXS. In addition, they provide insight into the molecular basis of the cognitive and allied defects in FXS and ASD and suggest multiple targets for clinical intervention.{$<$}/p{$><$}h3{$>$}PaperClip{$<$}/h3{$>$}},
