  author = {Vergauwen, Glenn and Dhondt, Bert and Deun, Jan Van and Smedt, Eva De and Berx, Geert and Timmerman, Evy and Gevaert, Kris and Miinalainen, Ilkka and Cocquyt, V{\'e}ronique and Braems, Geert and den Broecke, Rudy Van and Denys, Hannelore and Wever, Olivier De and Hendrix, An},
  month = jun,
  year = {2017},
  pages = {2704},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Honours\\Methods\\Protein quant\\Vergauwen et al_2017_Confounding factors of ultrafiltration and protein analysis in extracellular.pdf;C:\\Users\\charl\\Zotero\\storage\\ZPTE46J7\\s41598-017-02599-y.html}
