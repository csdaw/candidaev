  pmid = {29970457}
}

@article{thriene2018,
  title = {Combinatorial {{Omics Analysis Reveals Perturbed Lysosomal Homeostasis}} in {{Collagen VII}}-Deficient {{Keratinocytes}}},
  volume = {17},
