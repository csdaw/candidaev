  number = {23},
  journal = {Journal of Biological Chemistry},
  doi = {10.1074/jbc.M116.765669},
  author = {{Saint-Pol}, Julien and Billard, Martine and Dornier, Emmanuel and Eschenbrenner, Etienne and Danglot, Lydia and Boucheix, Claude and Charrin, St{\'e}phanie and Rubinstein, Eric},
