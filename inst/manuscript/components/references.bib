  pages = {8065},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Honours\\Fungal EVs\\Histo EVs\\Baltazar et al_2018_Concentration-dependent protein loading of extracellular vesicles released by.pdf;C:\\Users\\charl\\Zotero\\storage\\V37N6D5N\\s41598-018-25665-5.html}
