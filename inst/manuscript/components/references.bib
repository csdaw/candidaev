  month = may,
  year = {2018},
  keywords = {interactome capture,IRES uORF,mRNA translation,protein synthesis,RNA binding protein,terminal oligopyrimidine tract},
  pages = {e1465},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Cambridge application\\RBPs\\Review\\Harvey et al_2018_Trans-acting translational regulatory RNA binding proteins.pdf;C:\\Users\\charl\\Zotero\\storage\\L4MKRYMK\\wrna.html}
