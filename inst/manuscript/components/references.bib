@article{rida2006,
  title = {Yeast-to-{{Hyphal Transition Triggers Formin}}-Dependent {{Golgi Localization}} to the {{Growing Tip}} in {{Candida}} Albicans},
  volume = {17},
  issn = {1059-1524},
  abstract = {Rapid and long-distance secretion of membrane components is critical for hyphal formation in filamentous fungi, but the mechanisms responsible for polarized trafficking are not well understood. Here, we demonstrate that in Candida albicans, the majority of the Golgi complex is redistributed to the distal region during hyphal formation. Randomly distributed Golgi puncta in yeast cells cluster toward the growing tip during hyphal formation, remain associated with the distal portion of the filament during its extension, and are almost absent from the cell body. This restricted Golgi localization pattern is distinct from other organelles, including the endoplasmic reticulum, vacuole and mitochondria, which remain distributed throughout the cell body and hypha. Hyphal-induced positioning of the Golgi and the maintenance of its structural integrity requires actin cytoskeleton, but not microtubules. Absence of the formin Bni1 causes a hyphal-specific dispersal of the Golgi into a haze of finely dispersed vesicles with a sedimentation density no different from that of normal Golgi. These results demonstrate the existence of a hyphal-specific, Bni1-dependent cue for Golgi integrity and positioning at the distal portion of the hyphal tip, and suggest that filamentous fungi have evolved a novel strategy for polarized secretion, involving a redistribution of the Golgi to the growing tip.},
  number = {10},
  journal = {MBoC},
  doi = {10.1091/mbc.e06-02-0143},
  author = {Rida, Padmashree C.G. and Nishikawa, Akiko and Won, Gena Y. and Dean, Neta},
