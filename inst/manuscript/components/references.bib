  year = {2017},
  keywords = {Software engineering,Australian archaeology,Computer programming,Open science,Reproducible research},
  pages = {424-450},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Honours\\Model papers\\Research compendium\\Marwick_2017_Computational Reproducibility in Archaeological Research.pdf}
