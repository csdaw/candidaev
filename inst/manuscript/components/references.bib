  month = jan,
  year = {2006},
  keywords = {Candida albicans,Candidiasis,Endocytosis,Hyphae,Saccharomyces cerevisiae,Cell Adhesion,Cell Wall,Endothelial Cells,Epithelial Cells,Fungal Proteins,Humans,Mutation,Protein Transport},
  pages = {140-147},
