  month = may,
  year = {2013},
  keywords = {Exosome,Biodistribution,Lactadherin,luciferase},
  pages = {77-84},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Honours\\EV markers\\Mammalian\\Takahashi et al_2013_Visualization and in vivo tracking of the exosomes of murine melanoma B16-BL6.pdf;C:\\Users\\charl\\Zotero\\storage\\J9U8T37X\\S0168165613001648.html}
}

