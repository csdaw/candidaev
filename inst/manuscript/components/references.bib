  file = {C:\\Users\\charl\\OneDrive\\zotero\\Honours\\GFP fusions\\Luo et al_2013_Glycerol-3-Phosphate Dehydrogenase 2 Is a Novel Factor H–, Factor H–like.pdf;C:\\Users\\charl\\Zotero\\storage\\ECB5RSKR\\933267.html}
}

@article{smith2015,
