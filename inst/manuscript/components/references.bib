  keywords = {CD81,Tetraspanin-enriched microdomains,Palmitoylation,Cell entry,Hepatitis C virus},
  pages = {112-123},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Honours\\Tetraspanins\\CD81\\Zhu et al_2012_Significance of palmitoylation of CD81 on its association with.pdf;C:\\Users\\charl\\Zotero\\storage\\NIH8XWQ8\\S0042682212001596.html}
