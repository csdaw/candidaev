  number = {4},
  journal = {PLOS ONE},
  doi = {10.1371/journal.pone.0061940},
  author = {Palige, Katja and Linde, J{\"o}rg and Martin, Ronny and B{\"o}ttcher, Bettina and Citiulo, Francesco and Sullivan, Derek J. and Weber, Johann and Staib, Claudia and Rupp, Steffen and Hube, Bernhard and Morschh{\"a}user, Joachim and Staib, Peter},
  month = apr,
  year = {2013},
  keywords = {Candida,Candida albicans,Cell walls,Fluorescence microscopy,Fungal pathogens,Gene expression,Green fluorescent protein,orf19.6741,RNA sequencing},
  pages = {e61940},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Honours\\EV markers\\Candida 43\\Palige et al_2013_Global Transcriptome Sequencing Identifies Chlamydospore Specific Markers in.pdf;C:\\Users\\charl\\Zotero\\storage\\F3UZZMB7\\article.html}
