  year = {2015},
  keywords = {mass-limited samples,mass-spectrometry,microgram samples,proteins},
  pages = {3537-3563},
  file = {C:\\Users\\charl\\OneDrive\\zotero\\Honours\\Methods\\MS sample prep\\Feist_Hummon_2015_Proteomic Challenges.pdf;C:\\Users\\charl\\Zotero\\storage\\I4CNDQJJ\\3537.html}
}

@article{yu2014,
